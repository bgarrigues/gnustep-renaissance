<?xml version="1.0"?>
<!DOCTYPE gsmarkup>
<gsmarkup>

  <objects>
    
    <window title="Stepper Test" closable="no">
      <hbox>
        <label>Age:</label>
        <textField id="ageField" target="#ageStepper" action="takeIntValueFrom:">30</textField>
        <stepper id="ageStepper" autorepeat="yes" maxValue="99" target="#ageField" action="takeIntValueFrom:" intValue="#ageField.intValue" />
      </hbox>
    </window>
    
  </objects>
	
</gsmarkup>
