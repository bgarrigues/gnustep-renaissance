<?xml version="1.0"?>
<!DOCTYPE gsmarkup>

<gsmarkup>
  <objects>
    <panel title="Alert Panel">
      <vbox>
	<box hasBorder="no">
	  <label>Do you like the GNUstep Markup Language ?</label>
	</box>
	<hbox halign="right" type="proportional">
	  <button title="Yes" />
	  <button title="No" />
	  <button title="Quit" action="terminate:" />
	</hbox>
      </vbox>
    </panel>
  </objects>
</gsmarkup>