<?xml version="1.0"?>
<!DOCTYPE gsmarkup>

<gsmarkup>
  <objects>
    <window title="OutlineViewDemo">
      <vbox>
	<scrollView width="250" height="250">
	  <outlineView dataSource="#NSOwner" autosaveName="theOutline"
		       outlineColumn="0">
	    <tableColumn identifier="name" title="Class Name" 
			 minWidth="100"/>
	    <tableColumn identifier="instance" title="Instance Size"
			 minWidth="100"/>
	  </outlineView>
	</scrollView>

      </vbox>
    </window>
  </objects>
</gsmarkup>